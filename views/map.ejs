<%- include header.ejs %>
<%- include navbar.ejs %>

<div id="map">
    <div class="map_gmap"></div>
    <div class="map_circle">
        <div class="map_circle_inner_wrap">
            <div class="map_circle_inner"></div>
        </div>
        <div class="map_circle_outer"></div>
        <div class="map_circle_radar"></div>
    </div>
</div>

<div class="actions">
  <div class="wager">
    <input type="range" name="wager-slider" id="wager-slider" value="500" min="1" max="1000" data-highlight="true" />
  </div>
  <div class="search-area">
    Slide to Search
    <div id="search-slider"><i class="fa fa-arrow-right"></i></div>
    <div id="search-drop"></div>
  </div>
</div>

<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCKMNiwXFPxNyNczYX2j_Hc3KQWCuR_SBo&sensor=true"></script>
<script type="text/javascript" src="/js/jquery.throttledresize.js"></script>
<script type="text/javascript" src="/js/jquery.mousewheel.js"></script>
<script type="text/javascript">
    window.startingBalance = <%= user.balance %>;
</script>
<script type="text/javascript" src="/js/map.js"></script>
<%- include footer.ejs %>